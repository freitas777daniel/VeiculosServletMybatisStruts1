<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN"
          "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">
<struts-config>
       <action-mappings>
        <action path="/acao" type="action.ProprietarioAction">
	        <forward name="listar" path="/viewproprietarios.jsp"/>
	        <forward name="criar" path="/index.jsp"/>
	        <forward name="mostrarId" path="/editform.jsp"/>
	        <forward name="atualizar" path="/index.jsp"/>
	        <forward name="deletar" path="/index.jsp"/>
        </action>
        <action path="/acaoVeiculos" type="action.VeiculoAction">
	        <forward name="listar" path="/ViewVeiculo.jsp"/>
	        <forward name="criar" path="/index.jsp"/>
	        <forward name="mostrarId" path="/EditVeiculo.jsp"/>
	        <forward name="atualizar" path="/index.jsp"/>
	        <forward name="deletar" path="/index.jsp"/>
        </action>
    </action-mappings>
    
</struts-config>